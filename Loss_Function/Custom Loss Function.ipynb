   "metadata": {},
   "outputs": [],
   "source": [
    "K = tf.keras.backend\n",
    "\n",
    "def Categorical_Cross_Entropy():\n",
    "    def Loss_function(Y_True,Y_pred):\n",
    "        loss = K.categorical_crossentropy(Y_True,Y_pred,from_logits=False,axis = 3)\n",
    "        return tf.math.reduce_mean(loss)\n",
    "    return Loss_function"
   ]
